int x = (-5)*2+100;
int y = x % 5;
// y = 0
if(y){
	obliterate(y);
}else{
	// x = 90 / 18 = 5
	x = x / 18;
	
	// y = 0 + 5 = 5
	y = y + 5;
	
	// z = 5 * 2 - 5 = 5
	int z = x*2-y;
	
	// 5 - 5*2 = 5 - 10 = -5
	if(x-y*2 < 0){
		obliterate(z);
	}
}

